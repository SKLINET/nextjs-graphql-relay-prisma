{
  "presets":[
    "next/babel",
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    ["module-resolver", { "root": ["./src"] }],
    "inline-react-svg",
    "inline-dotenv",
    "relay"
  ],

  "env": {
    "development": {
      "plugins": [
        ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
      ]
    },
    "production": {
      "plugins": [
        ["styled-components", { "ssr": true, "displayName": false, "preprocess": false } ]
      ]
    }
  }
}
